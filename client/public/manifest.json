{
  "name": "sync cookies",
  "version": "0.1",
  "manifest_version": 2,
  "description": "sync cookies",
  "permissions": [
    "storage",
    "cookies",
    "<all_urls>"
  ],
  "icons": { 
    "16": "pechenie-16x16.png",
    "48": "pechenie-48x48.png",
    "128": "pechenie-128x128.png" },  
  "background": {
    "scripts": ["signalr.min.js", "chromeStorageTypes.js", "syncCookieClient.js", "startup.js", "cookiesOnChanged.js"],
    "persistent": true 
  },
  "browser_action": {
    "default_popup": "index.html"
  },
  "content_security_policy": "script-src 'self' 'sha256-A5S2zOz0DEczHfth+DWvuftVNd169Uo6nVlrplQZADg='; object-src 'self'"
}